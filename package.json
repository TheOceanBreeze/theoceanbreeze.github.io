{
  "name": "website",
  "version": "0.0.2",
  "description": "My personal website",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf docs",
    "build": "npm-run-all sass --parallel build:*",
    "start": "DEBUG=Eleventy* npm-run-all sass --parallel watch:*",
    "sass": "sass _sass/site.sass | npx postcss -o assets/css/style.css",
